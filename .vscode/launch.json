{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Debug Process",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Nest Debug",
      "protocol": "inspector",
      "sourceMaps": true,
      // "stopOnEntry": true,
      "console": "internalConsole",
      "program": "${workspaceFolder}/src/main.ts",
      // "preLaunchTask": "typescript",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
    }
  ],
  "compounds": []
}